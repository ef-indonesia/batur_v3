<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_visit"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/dimens_10">

    <View
        android:id="@+id/tv_no"
        android:layout_width="@dimen/dimens_5"
        android:layout_height="@dimen/dimens_10"
        android:layout_marginStart="@dimen/dimens_10"
        android:background="@color/light_transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="1" />

    <View
        android:id="@+id/tv_petani"
        android:layout_width="@dimen/dimens_50"
        android:layout_height="@dimen/dimens_20"
        android:layout_marginBottom="@dimen/dimens_5"
        android:background="@color/light_transparent"
        app:layout_constraintBottom_toTopOf="@id/tv_no_lahan"
        app:layout_constraintStart_toStartOf="@id/tv_no_lahan" />

    <View
        android:id="@+id/tv_no_lahan"
        android:layout_width="150dp"
        android:layout_height="@dimen/dimens_20"
        android:layout_marginStart="@dimen/dimens_20"
        android:background="@color/light_transparent"
        app:layout_constraintBottom_toBottomOf="@id/tv_no"
        app:layout_constraintStart_toEndOf="@id/tv_no"
        app:layout_constraintTop_toTopOf="@id/tv_no" />

    <View
        android:id="@+id/tv_komoditas"
        android:layout_width="200dp"
        android:layout_height="@dimen/dimens_20"
        android:layout_marginTop="@dimen/dimens_5"
        android:background="@color/light_transparent"
        app:layout_constraintStart_toStartOf="@id/tv_no_lahan"
        app:layout_constraintTop_toBottomOf="@id/tv_no_lahan" />

    <View
        android:id="@+id/tv_tgl_visit"
        android:layout_width="150dp"
        android:layout_height="@dimen/dimens_20"
        android:background="@color/light_transparent"
        app:layout_constraintBottom_toBottomOf="@id/tv_no_lahan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_no_lahan" />

</androidx.constraintlayout.widget.ConstraintLayout>
